{
  "name": "dns-audic-action",
  "version": "1.0.0",
  "description": "GitHub action to audit all DNS requests made by a workflow file",
  "main": "src/index.js",
  "scripts": {
    "build": "ncc build src/index.js --source-map --license licenses.txt",
    "build-test": "ncc build src/index.js --source-map --license licenses.txt -o dist-test",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "lint": "eslint src",
    "prepare": "husky install",
    "pre-commit": "npm run format:write && npm run build",
    "start": "node src/index.js",
    "test": "c8 jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cds-snc/dns-audic-action.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vercel/ncc": "0.36.1",
    "c8": "7.14.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-security": "1.7.1",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "jest-when": "3.5.2",
    "prettier": "2.8.8"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "execa": "^7.2.0",
    "ip": "^1.1.8"
  }
}
